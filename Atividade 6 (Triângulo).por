programa {
  funcao inicio() {
    inteiro l1, l2, l3
    escreva ("informe o lado 1: ")
    leia (l1)
    escreva ("informe o lado 2: ")
    leia (l2)
    escreva ("informe o lado 3: ")
    leia (l3)
    se ((l1 + l2) > l3){
      escreva ("isso pode formar um triângulo")
     }
    senao {
      escreva ("isso não pode ser um triângulo")
     }
  }
}